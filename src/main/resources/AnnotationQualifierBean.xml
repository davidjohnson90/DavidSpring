<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="studentProfile" class="com.david.spring.annotationBasedConfigurations.qualifierAnnotation.StudentProfile"></bean>

    <bean id="student1" class="com.david.spring.annotationBasedConfigurations.qualifierAnnotation.Student">
        <property name="name" value="Arun"></property>
        <property name="age" value="30"></property>
    </bean>

    <bean id="student2" class="com.david.spring.annotationBasedConfigurations.qualifierAnnotation.Student">
        <property name="name" value="David"></property>
        <property name="age" value="30"></property>
    </bean>
</beans>