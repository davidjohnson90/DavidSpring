<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="javaCollection" class="com.david.spring.injectingCollections.JavaCollection">

        <property name="addressList">
            <list>
                <value>INDIA</value>
                <value>INDIA</value>
                <value>CHINA</value>
                <value>USA</value>
                <value>USA</value>
            </list>
        </property>

        <property name="addressSet">
            <set>
                <value>INDIA</value>
                <value>INDIA</value>
                <value>CHINA</value>
                <value>USA</value>
                <value>USA</value>
            </set>
        </property>

        <property name="addressMap">
            <map>
                <entry key="1" value="INDIA"/>
                <entry key="2" value="INDIA"/>
                <entry key="3" value="CHINA"/>
                <entry key="4" value="USA"/>
                <entry key="5" value="USA"/>
            </map>
        </property>

        <property name="addressProperties">
            <props>
                <prop key="one">INDIA</prop>
                <prop key="two">INDIA</prop>
                <prop key="two">CHINA</prop>
                <prop key="three">USA</prop>
                <prop key="four">USA</prop>
            </props>
        </property>
    </bean>
</beans>